@model IEnumerable<GolfPro.Models.service_Setting>

@{
    ViewBag.Title = "allServices";
}

<h2>View All Services</h2>

<p>
    @Html.ActionLink("Add New Service ", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Specification)
        </th>
        <th>Edit </th>
        <th>Delete</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Specification)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
          </td>
        <td>
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
<a href="@Url.Action("DashboardSetting","Dashboard")" class="btn btn-warning">Back to Dashboard Working</a>